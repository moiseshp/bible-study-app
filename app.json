{
  "expo": {
    "name": "Devocionales Lince",
    "slug": "devocionales-lince",
    "version": "0.1.0-beta",
    "scheme": "bible-study",
    "platforms": ["android", "ios"],
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#18181b"
    },
    "ios": {
      "buildNumber": "1",
      "supportsTablet": true
    },
    "android": {
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#18181b"
      }
    },
    "plugins": ["expo-font"],
    "experiments": {
      "tsconfigPaths": true
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    }
  }
}
